{
  "cursor": {
    "rules": {
      "constitution": true,
      "languages": {
        "TypeScript": {
          "alias": "languages/ts"
        },
        "JavaScript": {
          "alias": "languages/js"
        },
        "vue": true
      },
      "roles/Translator": {
        "alias": "my-custom-translator"
      }
    },
    "destination": ".cursor/rules",
    "overwrite": false
  }
}
